    "apollo-server": "^3.12.1",
    "apollo-server-core": "^3.12.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "eslint": "^8.52.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.2",
    "jsonwebtoken": "^9.0.2",
